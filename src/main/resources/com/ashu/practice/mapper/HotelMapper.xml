<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ashu.practice.mapper.HotelMapper">
    <select id="selectByCityId" resultType="com.ashu.practice.domain.Hotel">
        select city_id, name, address, zip from hotel where city_id = #{id}
    </select>
</mapper>